{
  "expo": {
    "name": "Culture Preservation App",
    "slug": "culture-preservation-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "automatic",
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.yourcompany.culturepreservation"
    },
    "android": {
      "package": "com.yourcompany.culturepreservation",
      "permissions": [
        "CAMERA",
        "ACCESS_FINE_LOCATION",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE"
      ]
    },
    "web": {

    },
    "plugins": [
      "expo-location",
      "expo-media-library",
      "expo-image-picker"
    ],
    "extra": {
      "eas": {
        "projectId": "your-project-id"
      }
    }
  }
} 